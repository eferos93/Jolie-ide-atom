{
  "name": "jolie-ide",
  "main": "./lib/main.js",
  "version": "0.0.5",
  "description": "Jolie Language support for Atom IDE",
  "keywords": [
    "jolie",
    "completion",
    "lsp"
  ],
  "activationEvents": [
    "onLanguage:jolie"
  ],
  "categories": ["Programming Languages"],
  "repository": "https://github.com/atom/jolie-language-client",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient":"^0.9.9",
    "language-jolie":"^0.6.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "0.1.0": "provideAutocomplete"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  }
}
